{
  "dependencies": {
    "http-server": "^0.8.0",
    "jspm": "^0.15.7",
    "uglify": "^0.1.5"
  },
  "name": "whitesaveme",
  "version": "0.1.0",
  "description": "Whitesplainin' as a Service",
  "main": "config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "jspm install && jspm bundle-sfx 'lib/init' && uglify -s site/build.js -o site/whitesaveme.js && uglify -cs site/whitesaveme.css -o site/whitesaveme.min.css"
  },
  "author": "Dmytri Kleiner <dk@telekommunisten.net>",
  "license": "WTFPL",
  "jspm": {
    "directories": {
      "baseURL": "site"
    },
    "dependencies": {
      "clean-css": "npm:clean-css@^3.3.3",
      "css": "github:systemjs/plugin-css@^0.1.13",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.88",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.88"
    }
  }
}
